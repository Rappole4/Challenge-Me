<p id="notice"><%= notice %></p>

<h1>Comp Challenges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comp_challenges.each do |comp_challenge| %>
      <tr>
        <td><%= link_to 'Show', comp_challenge %></td>
        <td><%= link_to 'Edit', edit_comp_challenge_path(comp_challenge) %></td>
        <td><%= link_to 'Destroy', comp_challenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comp Challenge', new_comp_challenge_path %>
