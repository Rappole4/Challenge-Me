<p id="notice"><%= notice %></p>

<h1>Compchallenges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compchallenges.each do |compchallenge| %>
      <tr>
        <td><%= link_to 'Show', compchallenge %></td>
        <td><%= link_to 'Edit', edit_compchallenge_path(compchallenge) %></td>
        <td><%= link_to 'Destroy', compchallenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compchallenge', new_compchallenge_path %>
